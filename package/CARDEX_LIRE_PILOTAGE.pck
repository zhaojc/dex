procedure sp_l_classement_dossiers(rc1 IN OUT DTS_CLASSEMENT_DOSSIERS) is
begin
  open rc1 for SELECT
    EN.I_EN_CLE, EN.V_EN_ENTITE,
    GE.I_GE_CLE, TR1.V_TR_DESCRIPTION, NA.I_NA_CLE,
	TR2.V_TR_DESCRIPTION, TY.I_TY_CLE, TR3.V_TR_DESCRIPTION,
	CA.I_CA_CLE, TR4.V_TR_DESCRIPTION
  FROM
    EN_ENTITE EN,
    GE_GENRE GE,
    NA_NATURE NA,
    TY_TYPE TY,
    CA_CATEGORIE CA,
    TR_TRADUCTION TR1,
    TR_TRADUCTION TR2,
    TR_TRADUCTION TR3,
    TR_TRADUCTION TR4
  WHERE
    ( EN.I_EN_CLE=GE.I_GE_ENTITE ) AND
    ( GE.I_GE_CLE=NA.I_GE_CLE ) AND
    ( GE.I_GE_CLE=TR1.L_TR_CLE ) AND
    ( NA.I_NA_CLE=TY.I_NA_CLE ) AND
    ( NA.I_NA_CLE=TR2.L_TR_CLE ) AND
    ( TY.I_TY_CLE=CA.I_TY_CLE ) AND
    ( TY.I_TY_CLE=TR3.L_TR_CLE ) AND
    ( CA.I_CA_CLE=TR4.L_TR_CLE ) AND
    ( TR1.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
    ( TR2.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
    ( TR3.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
    ( TR4.I_LA_CLE = CARDEX_SPECIAL.g_langue);
END SP_L_CLASSEMENT_DOSSIERS;
--Classement des dossiers filtrés par site et triés.
procedure sp_l2_classement_dossiers(rc1 IN OUT DTS_CLASSEMENT_DOSSIERS) is
begin
  open rc1 for SELECT
    EN.I_EN_CLE, EN.V_EN_ENTITE,
    GE.I_GE_CLE, TR1.V_TR_DESCRIPTION as "GENRE", NA.I_NA_CLE,
	TR2.V_TR_DESCRIPTION as "NATURE", TY.I_TY_CLE, TR3.V_TR_DESCRIPTION as "TYPE",
	CA.I_CA_CLE, TR4.V_TR_DESCRIPTION as "CATEGORIE"
  FROM
    EN_ENTITE EN,
    GE_GENRE GE,
    NA_NATURE NA,
    TY_TYPE TY,
    CA_CATEGORIE CA,
    TR_TRADUCTION TR1,
    TR_TRADUCTION TR2,
    TR_TRADUCTION TR3,
    TR_TRADUCTION TR4
  WHERE
    ( EN.I_EN_CLE=GE.I_GE_ENTITE ) AND
    ( GE.I_GE_CLE=NA.I_GE_CLE ) AND
    ( GE.I_GE_CLE=TR1.L_TR_CLE ) AND
    ( NA.I_NA_CLE=TY.I_NA_CLE ) AND
    ( NA.I_NA_CLE=TR2.L_TR_CLE ) AND
    ( TY.I_TY_CLE=CA.I_TY_CLE ) AND
    ( TY.I_TY_CLE=TR3.L_TR_CLE ) AND
    ( CA.I_CA_CLE=TR4.L_TR_CLE ) AND
    ( TR1.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
    ( TR2.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
    ( TR3.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
    ( TR4.I_LA_CLE = CARDEX_SPECIAL.g_langue) AND
	en.i_en_cle in (select s.i_en_cle from si_site s where s.l_si_cle = CARDEX_SPECIAL.g_site)
	order by EN.V_EN_ENTITE,TR1.V_TR_DESCRIPTION,TR2.V_TR_DESCRIPTION,TR3.V_TR_DESCRIPTION,TR4.V_TR_DESCRIPTION;
END SP_L2_CLASSEMENT_DOSSIERS;
