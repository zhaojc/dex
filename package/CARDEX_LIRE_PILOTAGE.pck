--Recherche de villes avec un critère.
procedure SP_L2_VI_VILLE(NEW_PROVINCE IN varchar2,NEW_VILLE IN varchar2, rc1 IN OUT DTS2_VI_VILLE) is
PROVINCE varchar2(100) := '%'; --Caractère générique pour rechercher tous les enregistrements.
VILLE varchar2(200) := '%';
begin
If NOT NEW_PROVINCE IS NULL then
   PROVINCE := NEW_PROVINCE;
End if;
If NOT NEW_VILLE IS NULL then
   VILLE := NEW_VILLE;
End if;
     open rc1 for select
         P.L_VI_CLE,
         P.L_PR_CLE,
         P.V_VI_VILLE,
         P.V_VI_CREE_PAR,
         P.D_VI_DATE_CREATION,
         P.V_VI_MODIFIE_PAR,
         P.D_VI_DATE_MODIFICATION
     from VI_VILLE P
	   where to_char(l_pr_cle)like PROVINCE and lower(v_vi_ville) like VILLE
     order by NLSSORT(P.V_VI_VILLE,'nls_sort = french');
END SP_L2_VI_VILLE;
