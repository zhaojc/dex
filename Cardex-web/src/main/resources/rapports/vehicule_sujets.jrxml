 from su_sujet s, ldd_lien_dossier l, ve_vehicule ve, tr_traduction trsx, tr_traduction trra, tr_traduction trnt, tr_traduction trls
 where ((s.l_su_cle = l.L_LDD_DOSSIER_ASSOCIE and l.c_ldd_genre = 'SU' and l.l_ldd_site = s.l_si_cle 
and l.l_do_cle = ve.l_ve_cle and l.l_do_site = ve.l_si_cle and l.c_do_genre = 'VE') OR s.l_su_cle = l.l_do_cle 
and s.l_si_cle = l.l_do_site and l.c_do_genre = 'SU' and ve.l_ve_cle = l.l_ldd_dossier_associe and ve.l_si_cle = l.l_ldd_site
and l.c_ldd_genre = 'VE') and ve.l_ve_cle = $P{VEHICULE_CLE} and ve.l_si_cle = $P{VEHICULE_SITE}
and s.i_sx_cle = trsx.l_tr_cle(+) and trsx.i_la_cle(+) = 1
and s.i_ra_cle = trra.l_tr_cle(+) and trra.i_la_cle(+) = 1
and s.i_nt_cle = trnt.l_tr_cle(+) and trnt.i_la_cle(+) = 1
and s.i_ls_cle = trls.l_tr_cle(+) and trls.i_la_cle(+) = 1]]>
