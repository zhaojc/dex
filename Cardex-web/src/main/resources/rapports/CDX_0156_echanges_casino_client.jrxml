	<queryString>
		<![CDATA[select s.v_si_site Site, t.v_tr_description Type, sum(c.r_cn_montant) Total
  from cardex.cn_consignation c, cardex.si_site s, cardex.tr_traduction t
 where trunc(C.D_CN_DATE_CREATION) between
       TO_DATE($P{DATE_DEBUT}, 'YYYY-MM-DD')
   AND TO_DATE($P{DATE_FIN}, 'YYYY-MM-DD')
   and s.l_si_cle = c.l_si_cle
   and c.i_tn_cle in (25616, 24924, 25617, 24923)
   and c.i_tn_cle = t.l_tr_cle
   and t.i_la_cle = 1
 group by s.v_si_site, t.v_tr_description
 order by s.v_si_site, t.v_tr_description]]>
