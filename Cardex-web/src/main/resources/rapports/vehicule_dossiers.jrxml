	<parameter name="VEHICULE_CLE" class="java.math.BigDecimal"/>
	<parameter name="VEHICULE_SITE" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select distinct d.v_do_ancienne_reference,
                d.v_do_numero_dossier,
                trse.v_tr_description as "SEVERITE",
                trcc.v_tr_description as "CONFIDENTIALITE",
                trna.v_tr_description as "NATURE",
                trty.v_tr_description as "TYPE",
                trca.v_tr_description as "CATEGORIE",
                trst.v_tr_description as "STATUT",
                d.d_do_date_debut,
                d.d_do_date_fin,
                d.v_do_assigne_a
  from v_do_dossier_ca_ty d,
       ldd_lien_dossier   l,
       ve_vehicule        ve,
       tr_traduction      trse,
       tr_traduction      trcc,
       tr_traduction      trna,
       tr_traduction      trty,
       tr_traduction      trca,
       tr_traduction      trst
 where d.l_do_cle = l.l_do_cle
   and d.l_si_cle = l.l_do_site
   and l.c_do_genre = 'DO'
   and ve.l_ve_cle = l.l_ldd_dossier_associe
   and ve.l_si_cle = l.l_ldd_site
   and l.c_ldd_genre = 'VE'
   and ve.l_ve_cle = $P{VEHICULE_CLE}
   and ve.l_si_cle = $P{VEHICULE_SITE}
   and d.i_se_cle = trse.l_tr_cle(+)
   and trse.i_la_cle(+) = 1
   and d.i_cc_cle = trcc.l_tr_cle
   and trcc.i_la_cle = 1
   and d.i_na_cle = trna.l_tr_cle
   and trna.i_la_cle = 1
   and d.i_ty_cle = trty.l_tr_cle
   and trty.i_la_cle = 1
   and d.i_ca_cle = trca.l_tr_cle
   and trca.i_la_cle = 1
   and d.i_st_cle = trst.l_tr_cle
   and trst.i_la_cle = 1
   and d.i_cc_cle <> 14920]]>
	</queryString>
